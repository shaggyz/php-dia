{
    "name": "shaggyz/php-dia",
    "description": "Diagram generator from php source code for GNU Dia.",
    "type": "program",
    "license": "GNU GPL v2",
    "autoload": {
        "psr-0": {
            "": "src/"
        }
    },
    "authors": [
        {
            "name": "NicolÃ¡s Daniel Palumbo",
            "email": "n@xinax.net"
        }
    ],
    "require": {
        "php": ">=7.0.22",
        "twig/twig": "^2.0",
        "nikic/php-parser": "v4.0.0alpha1",
        "symfony/console": "^3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "macfja/phar-builder": "^0.2.6"
    },
    "extra": {
        "phar-builder": {
            "compression": "GZip",
            "name": "phpdia.phar",
            "output-dir": "build/",
            "entry-point": "phpdia.php",
            "include": ["src"],
            "include-dev": false,
            "skip-shebang": false
        }
    }

}
